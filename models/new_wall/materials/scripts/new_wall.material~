material vrc/new_wall_pebbles
{
	receive_shadows off
	technique
	{
		pass
		{
			texture_unit
			{
				texture pebbles.jpg
				filtering anistropic
				max_anisotropy 16
				scale 1 1
			}
		}
	}
}

